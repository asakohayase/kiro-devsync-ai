{
  "mcpServers": {
    "supabase": {
      "command": "uvx",
      "args": ["supabase-mcp-server@latest"],
      "env": {
        "SUPABASE_URL": "your-supabase-url-here",
        "SUPABASE_KEY": "your-supabase-anon-key-here"
      },
      "disabled": false,
      "autoApprove": ["query_database", "list_tables", "describe_table"]
    }
  }
}